#!/bin/bash
DIR=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
source $DIR/filenames.sh

#$DIR/download_corpus
#python $DIR/modify_corpus.py
$HOME/trunk/word2vec -min-count 200 -hs 1 -window 10 -size 100 -cbow 1 -debug 2 -threads 8 -iter 10 -binary 1 -output $vectors_binary -train $corpus_modified
$HOME/trunk/word2vec -min-count 200 -hs 0 -negative 5 -window 10 -size 100 -cbow 1 -debug 2 -threads 8 -iter 10 -binary 1 -output "$vectors_binary".ns -train $corpus_modified
